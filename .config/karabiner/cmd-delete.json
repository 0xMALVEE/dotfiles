{
  "description": "Command + Delete deletes only one word instead of whole line",
  "manipulators": [
    {
      "from": {
        "key_code": "delete_or_backspace",
        "modifiers": {
          "mandatory": ["left_command"],
          "optional": ["any"]
        }
      },
      "to": [
        {
          "key_code": "delete_or_backspace",
          "modifiers": ["option"]
        }
      ],
      "type": "basic"
    }
  ]
}
